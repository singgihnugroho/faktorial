package javaapplication1;

/**
 *
 * @author Asisten
 */
public class hitung {
    public static int hsl(int nilai){
        int i, tot=1;
        for(i=1; i<=nilai; i++){
            tot=tot*i;
        }
        System.out.println("faktorial dari "+nilai+"!: " );
        return 0;
        
        }
    };
class faktorial{
    public static void main(String [] args){
        int hasil;
        hitung ht=new hitung();
        hasil=hitung.hsl(5);
        System.out.println("hasil faktorial =  " +hasil);
        
    }
}
